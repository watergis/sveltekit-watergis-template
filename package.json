{
	"name": "sveltekit-maplibre-boilerplate",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "rimraf build && vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "lefthook install",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .prettierignore --check --plugin-search-dir=. . && eslint --ignore-path .prettierignore .",
		"format": "prettier --ignore-path .prettierignore --write --plugin-search-dir=. .",
		"preinstall": "npx only-allow pnpm"
	},
	"devDependencies": {
		"@arkweid/lefthook": "^0.7.7",
		"@fortawesome/free-solid-svg-icons": "^6.2.1",
		"@mapbox/mapbox-gl-geocoder": "^5.0.1",
		"@popperjs/core": "^2.11.6",
		"@sveltejs/adapter-static": "^1.0.0",
		"@sveltejs/kit": "^1.0.1",
		"@types/events": "^3.0.0",
		"@types/node": "^18.11.17",
		"@typescript-eslint/eslint-plugin": "^5.47.0",
		"@typescript-eslint/parser": "^5.47.0",
		"@watergis/mapbox-gl-area-switcher": "^1.0.1",
		"@watergis/maplibre-center-icon": "^0.1.8",
		"@watergis/svelte-collapsible-panel": "^0.0.14",
		"@watergis/svelte-maplibre-attribute-popup": "^0.0.16",
		"@watergis/svelte-maplibre-export": "^0.1.20",
		"@watergis/svelte-maplibre-legend": "^0.1.19",
		"@watergis/svelte-maplibre-measure": "^0.1.18",
		"@watergis/svelte-maplibre-menu": "^0.1.17",
		"@watergis/svelte-maplibre-share": "^0.1.13",
		"@watergis/svelte-maplibre-style-switcher": "^0.1.15",
		"@watergis/svelte-maplibre-valhalla": "^0.0.14",
		"buffer": "^6.0.3",
		"bulma": "^0.9.4",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"events": "^3.3.0",
		"maplibre-gl": "^2.4.0",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"rimraf": "^3.0.2",
		"sass": "^1.57.1",
		"svelte": "^3.55.0",
		"svelte-check": "^3.0.1",
		"svelte-fa": "^3.0.3",
		"svelte-preprocess": "^5.0.0",
		"tslib": "^2.4.1",
		"typescript": "~4.9.4",
		"vite": "^4.0.3"
	},
	"type": "module"
}
