{
	"name": "sveltekit-maplibre-boilerplate",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "rimraf build && vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "lefthook install",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .prettierignore --check --plugin-search-dir=. . && eslint --ignore-path .prettierignore .",
		"format": "prettier --ignore-path .prettierignore --write --plugin-search-dir=. .",
		"preinstall": "npx only-allow pnpm"
	},
	"devDependencies": {
		"@arkweid/lefthook": "^0.7.7",
		"@sveltejs/adapter-static": "^1.0.0-next.46",
		"@sveltejs/kit": "^1.0.0-next.525",
		"@typescript-eslint/eslint-plugin": "^5.41.0",
		"@typescript-eslint/parser": "^5.41.0",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.8.0",
		"rimraf": "^3.0.2",
		"sass": "^1.55.0",
		"svelte": "^3.52.0",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.0",
		"typescript": "~4.8.4",
		"vite": "^3.1.8"
	},
	"dependencies": {
		"@turf/distance": "^6.5.0",
		"@watergis/mapbox-gl-area-switcher": "^1.0.1",
		"@watergis/svelte-maplibre-attribute-popup": "^0.0.7",
		"@watergis/svelte-maplibre-export": "^0.1.6",
		"@watergis/svelte-maplibre-legend": "^0.1.0",
		"@watergis/svelte-maplibre-menu": "^0.1.2",
		"@watergis/svelte-maplibre-search": "^0.1.2",
		"@watergis/svelte-maplibre-share": "^0.1.2",
		"@watergis/terrain-rgb": "^1.1.8",
		"maplibre-gl": "^2.4.0"
	},
	"type": "module"
}
