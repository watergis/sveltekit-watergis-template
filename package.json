{
	"name": "sveltekit-maplibre-boilerplate",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "rimraf build && vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "lefthook install",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .prettierignore --check --plugin-search-dir=. . && eslint --ignore-path .prettierignore .",
		"format": "prettier --ignore-path .prettierignore --write --plugin-search-dir=. .",
		"preinstall": "npx only-allow pnpm"
	},
	"devDependencies": {
		"@arkweid/lefthook": "^0.7.7",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.44.0",
		"@typescript-eslint/parser": "^5.44.0",
		"bulma": "^0.9.4",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"rimraf": "^3.0.2",
		"sass": "^1.56.1",
		"svelte": "^3.53.1",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.1",
		"typescript": "~4.9.3",
		"vite": "^3.2.4"
	},
	"dependencies": {
		"@watergis/mapbox-gl-area-switcher": "^1.0.1",
		"@watergis/maplibre-center-icon": "^0.1.3",
		"@watergis/svelte-collapsible-panel": "^0.0.8",
		"@watergis/svelte-maplibre-attribute-popup": "^0.0.11",
		"@watergis/svelte-maplibre-export": "^0.1.15",
		"@watergis/svelte-maplibre-legend": "^0.1.8",
		"@watergis/svelte-maplibre-measure": "^0.1.11",
		"@watergis/svelte-maplibre-menu": "^0.1.10",
		"@watergis/svelte-maplibre-search": "^0.1.4",
		"@watergis/svelte-maplibre-share": "^0.1.8",
		"@watergis/svelte-maplibre-style-switcher": "^0.1.8",
		"@watergis/svelte-maplibre-valhalla": "^0.0.9",
		"maplibre-gl": "^2.4.0"
	},
	"type": "module"
}
