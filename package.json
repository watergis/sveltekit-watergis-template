{
	"name": "sveltekit-maplibre-boilerplate",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "rm -rf build && vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "husky install && npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .prettierignore --check --plugin-search-dir=. . && eslint --ignore-path .prettierignore .",
		"format": "prettier --ignore-path .prettierignore --write --plugin-search-dir=. .",
		"deploy": "npx gh-pages -d build -t true"
	},
	"devDependencies": {
		"@fortawesome/free-solid-svg-icons": "^6.1.2",
		"@geoffcox/svelte-splitter": "^1.0.1",
		"@smui-extra/accordion": "^6.0.0",
		"@smui/button": "^6.0.0",
		"@smui/card": "^6.0.0",
		"@smui/checkbox": "^6.0.0",
		"@smui/data-table": "^6.0.0",
		"@smui/dialog": "^6.0.0",
		"@smui/drawer": "^6.0.0",
		"@smui/form-field": "^6.0.0",
		"@smui/icon-button": "^6.0.0",
		"@smui/list": "^6.0.0",
		"@smui/select": "^6.0.0",
		"@smui/snackbar": "^6.0.0",
		"@smui/tab": "^6.0.0",
		"@smui/tab-bar": "^6.0.0",
		"@smui/textfield": "^6.0.0",
		"@smui/tooltip": "^6.0.0",
		"@smui/top-app-bar": "^6.0.0",
		"@sveltejs/adapter-static": "^1.0.0-next.39",
		"@sveltejs/kit": "^1.0.0-next.405",
		"@types/file-saver": "^2.0.5",
		"@typescript-eslint/eslint-plugin": "^5.33.0",
		"@typescript-eslint/parser": "^5.33.0",
		"eslint": "^8.21.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"gh-pages": "^4.0.0",
		"husky": "^8.0.1",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"smui-theme": "^6.0.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-fa": "^3.0.3",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.0",
		"typescript": "~4.7.4",
		"vite": "^3.0.7"
	},
	"type": "module",
	"dependencies": {
		"@maplibre/maplibre-gl-style-spec": "^17.0.0",
		"@turf/distance": "^6.5.0",
		"@watergis/legend-symbol": "^0.2.2",
		"@watergis/mapbox-gl-area-switcher": "^1.0.1",
		"@watergis/terrain-rgb": "^1.1.8",
		"D": "^1.0.0",
		"fabric": "^5.2.1",
		"file-saver": "^2.0.5",
		"file-server": "^2.2.0",
		"image-clipper": "^0.4.4",
		"jspdf": "^2.5.1",
		"maplibre-gl": "^2.2.1",
		"simple-svelte-autocomplete": "^2.4.0"
	}
}
