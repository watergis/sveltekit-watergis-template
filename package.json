{
	"name": "sveltekit-maplibre-boilerplate",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "rm -rf build && svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync && husky install && npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .prettierignore --check --plugin-search-dir=. . && eslint --ignore-path .prettierignore .",
		"format": "prettier --ignore-path .prettierignore --write --plugin-search-dir=. .",
		"deploy": "npx gh-pages -d build -t true"
	},
	"devDependencies": {
		"@fortawesome/free-solid-svg-icons": "^6.1.1",
		"@geoffcox/svelte-splitter": "^1.0.1",
		"@smui-extra/accordion": "^6.0.0-beta.16",
		"@smui/button": "^6.0.0-beta.16",
		"@smui/card": "^6.0.0-beta.16",
		"@smui/checkbox": "^6.0.0-beta.16",
		"@smui/data-table": "^6.0.0-beta.16",
		"@smui/dialog": "^6.0.0-beta.16",
		"@smui/drawer": "^6.0.0-beta.16",
		"@smui/form-field": "^6.0.0-beta.16",
		"@smui/icon-button": "^6.0.0-beta.16",
		"@smui/list": "^6.0.0-beta.16",
		"@smui/select": "^6.0.0-beta.16",
		"@smui/tab": "^6.0.0-beta.16",
		"@smui/tab-bar": "^6.0.0-beta.16",
		"@smui/textfield": "^6.0.0-beta.16",
		"@smui/tooltip": "^6.0.0-beta.16",
		"@smui/top-app-bar": "^6.0.0-beta.16",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@types/file-saver": "^2.0.5",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"gh-pages": "^3.2.3",
		"husky": "^7.0.0",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"smui-theme": "^6.0.0-beta.16",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-fa": "^2.4.0",
		"svelte-preprocess": "^4.10.1",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"type": "module",
	"dependencies": {
		"@maplibre/maplibre-gl-style-spec": "^15.0.0",
		"@turf/distance": "^6.5.0",
		"@watergis/legend-symbol": "^0.2.2",
		"@watergis/mapbox-gl-area-switcher": "^1.0.1",
		"@watergis/terrain-rgb": "^1.1.8",
		"D": "^1.0.0",
		"fabric": "^5.2.1",
		"file-saver": "^2.0.5",
		"file-server": "^2.2.0",
		"image-clipper": "^0.4.4",
		"jspdf": "^2.5.1",
		"maplibre-gl": "^2.2.0-pre.2",
		"simple-svelte-autocomplete": "^2.4.0"
	}
}
